---
import Heading from "./Heading.astro";
import Button from "./Button.astro";
const heading = { tagline: 'Contactanos', headingText: 'Comencemos tu proyecto ahora',descriptionText: 'Envíanos un mensaje para comenzar a cotizar tu proyecto. '}
---


<section class="py-20 text-center border border-gray">
  <div class="container">
    <div class="mb-8">
      <Heading tagline={heading.tagline} headingText={heading.headingText} descriptionText={heading.descriptionText} />
    </div>
    <Button text="Solicita Cotización" href="/contacto"/> 
  </div>
    
</section>